<?xml version="1.0" encoding="utf-8" ?>
<Defs>



<ThingDef ParentName="SK_ResourceBase">
		<defName>Trap2_Explosive</defName>
		<label>Box with mines</label>
		<description>Box min.Explosive</description>
		<graphicData>
			<texPath>Things/Building/MinBoxHE</texPath>
		   <graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>(1.2,1)</drawSize>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<fillPercent>0.35</fillPercent>
		<stackLimit>6</stackLimit>
		<useHitPoints>true</useHitPoints>
		<techLevel>Spacer</techLevel>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>350</MarketValue>
			<DeteriorationRate>0.02</DeteriorationRate>
			<Flammability>5</Flammability>
			<Mass>3.1</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
		<li Class="CompProperties_Explosive">
				<explosiveRadius>3.4</explosiveRadius>
				<explosiveDamageType>Flame</explosiveDamageType>
				<explosiveExpandPerStackcount>0.2</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<wickTicks>
					<min>5</min>
					<max>10</max>
				</wickTicks>
			</li>
		</comps>
		<thingCategories>
			<li>Grenades</li>
		</thingCategories>
	</ThingDef>
	
	
